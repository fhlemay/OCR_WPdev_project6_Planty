{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#3b8e46",
					"name": "primary",
					"slug": "primary"
				},
				{
					"color": "#d2776a",
					"name": "secondary",
					"slug": "secondary"
				},
				{
					"color": "#dc9f96",
					"name": "Call to action",
					"slug": "cta"
				},
				{
					"color": "#e0b9b4",
					"name": "Call to action 2",
					"slug": "ctatwo"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#ece2da",
					"name": "Base",
					"slug": "base"
				}
			]
		},
		"layout": {
			"contentSize": "1440px",
			"wideSize": "1440px"
		},
		"spacing": {
			"blockGap": false,
			"margin": false,
			"padding": false,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/syne_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/syne_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/syne_normal_800.ttf"
							]
						}
					],
					"fontFamily": "Syne",
					"slug": "syne"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"color": {
					"background": "var(--wp--preset--color--ctatwo)"
				},
				"spacing": {
					"padding": {
						"bottom": "9px",
						"left": "56px",
						"right": "57px",
						"top": "9px"
					}
				},
				"typography": {
					"fontSize": "15px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "19px"
				}
			},
			"core/group": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--0)"
				}
			},
			"core/navigation": {
				"elements": {
					"button": {
						"border": {
							"radius": "0px"
						},
						"color": {
							"background": "var(--wp--preset--color--cta)"
						},
						"spacing": {
							"padding": {
								"bottom": "31px",
								"left": "61px",
								"right": "61px",
								"top": "30px"
							}
						},
						"typography": {
							"fontSize": "16px",
							"fontStyle": "normal",
							"fontWeight": "600"
						}
					},
					"link": {
						":hover": {
							"typography": {
								"fontSize": "13px",
								"fontWeight": "700",
								"textDecoration": "none"
							}
						},
						"typography": {
							"fontSize": "16px",
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400",
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "38px",
					"fontWeight": "800",
					"lineHeight": "46px",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "30px",
					"fontWeight": "800",
					"lineHeight": "36px",
					"textTransform": "uppercase"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "30px",
					"fontWeight": "700",
					"lineHeight": "36px"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				"typography": {
					"fontSize": "16px",
					"lineHeight": "19px",
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--syne)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json"
}