        "typography": {
          "fontSize": "16px",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "var(--wp--preset--spacing--0)"
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--syne)"
