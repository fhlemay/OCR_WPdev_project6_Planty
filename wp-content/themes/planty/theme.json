{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/syne_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/syne_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Syne",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/syne_normal_800.ttf"
							]
						}
					],
					"fontFamily": "Syne",
					"slug": "syne"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/group": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--30)"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
							  "fontSize": "13px",	
                "fontWeight": "700",
                "textDecoration": "none"
							}
						},
						"typography": {
              "fontSize": "16px",
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)",
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--syne)"
				}
			},
			"link": {
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        },
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--syne)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json"
}
